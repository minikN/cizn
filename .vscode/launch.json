            "name": "help",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "/home/db/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229,
            "program": "${workspaceFolder}/src/app.ts",
            "args": ["--help"],
            "env": {
                // "CIZN_PACKAGE_MANAGER": "bla"
            },
            "cwd": "${workspaceRoot}",
